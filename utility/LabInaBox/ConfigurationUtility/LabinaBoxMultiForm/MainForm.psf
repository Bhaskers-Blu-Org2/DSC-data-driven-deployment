else { $modulepath = Join-Path -Path $InstallLocation -ChildPath "modules\LabInaBox.psm1"}

import-module $modulepath

$formLabInABox_Load = {
	$InstallLocation = Get-Location
	
	if ($InstallLocation.ToString() -eq "D:\Git\DSC-data-driven-deployment\utility\LabInaBox\ConfigurationUtility\LabinaBoxMultiForm")
	{
		$modulepath = 'D:\Git\DSC-data-driven-deployment\utility\LabInaBox\modules\LabInaBox.psm1'
	}
	else { $modulepath = Join-Path -Path $InstallLocation -ChildPath "modules\LabInaBox.psm1" }
	
	import-module $modulepath	# remove all pages
	$script:savedPages = @{ }
	
	
