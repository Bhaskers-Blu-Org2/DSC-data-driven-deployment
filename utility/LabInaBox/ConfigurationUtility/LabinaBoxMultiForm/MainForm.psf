$openToolStripMenuItem_Click = {
	$openfiledialog.InitialDirectory = "$InstallLocation\LabConfig"
	$openfiledialog.FileName = "*.json"
	$openfiledialog.ShowDialog()
	
	$MyMainConfig = Get-Content $($openfiledialog.FileName) | ConvertFrom-Json | Convert-PSObjectToHashtable

	$LabType = $($MyMainConfig.LabType)

	If ($LabType -eq "AzureLab")
	{
		$MyDataTable = New-Object System.Data.DataTable
		$MyDataTable.Columns.Add('Server')
		$MyDataTable.Columns.Add('AzurePublisher')
		$MyDataTable.Columns.Add('AzureOffer')
		$MyDataTable.Columns.Add('AzureSku')
		$MyDataTable.Columns.Add('OS')
		$MyDataTable.Columns.Add('VMUserName')
		$MyDataTable.Columns.Add('VMPassword')
		
		$radiobuttonAzureLab.Checked = $true
		if (!$MyMainConfig.CertSubject)
		{
			Set-Location Cert:\CurrentUser\My
			$MyCerts = Get-ChildItem | Where-Object { $_.FriendlyName -NE "" } | select FriendlyName
			
			foreach ($Cert in $MyCerts.friendlyname)
			{
				Update-ComboBox $cb_CertificateList $Cert -Append
			}
		}
		else{$cb_CertificateList.SelectedItem = $MyMainConfig.CertSubject}
		$cb_CertificateList.SelectedItem = $MyMainConfig.CertSubject
		$tb_LabMachinePrefix.Text = $MyMainConfig.LabPrefix
		$tb_ApplicationID.Text = $MyMainConfig.ApplicationID
		$tb_tenantId.Text = $MyMainConfig.tenantId
		$tb_AzureLocation.Text = $MyMainConfig.Location
		$tb_AzureVMSize.Text = $MyMainConfig.Size
		$tb_SubnetAddress.Text = $MyMainConfig.SubnetAddress
		$tb_VnetAddress.Text = $MyMainConfig.VnetAddress

		$ServersToLoop = $MyMainConfig.DomainJoinServer.keys
		foreach ($server in $ServersToLoop)
		{
			$row = $MyDataTable.NewRow()
			$row["Server"] = $Server
			$row["AzurePublisher"]= $MyMainConfig.DomainJoinServer.$($server).AzurePublisher
			$row["AzureOffer"] = $MyMainConfig.DomainJoinServer.$($server).AzureOffer
			$row["AzureSku"] = $MyMainConfig.DomainJoinServer.$($server).AzureSku
			$row["OS"] = $MyMainConfig.DomainJoinServer.$($server).OS
			$row["VMUserName"] = $MyMainConfig.DomainJoinServer.$($server).VMUserName
			$row["VMPassword"] = $MyMainConfig.DomainJoinServer.$($server).VMPassword
			$MyDataTable.Rows.Add($row)
		}
		
		Load-DataGridView -DataGridView $Serverdatagridview1 -Item $MyDataTable -Verbose
	}
	
	if (($LabType -eq "HyperVLab") -or ($LabType -eq $null))
	{
		$radiobuttonHyperVLab.Checked = $true
		$tb_LabMachinePrefix.Text = $MyMainConfig.DomainName
		$tb_LocalAdminPass.Text = $MyMainConfig.LocalAdminPass
		$tb_DomainAdminPass.Text = $MyMainConfig.DomainAdminPass
		$tb_SysPrepDriveName.Text = $MyMainConfig.SysPrepDriveName
		$cb_SwitchType.SelectedItem = $MyMainConfig.SwitchType
		$tb_NetworkIpRange.Text = $MyMainConfig.NetworkIPRange
		$tb_SwitchName.Text = $MyMainConfig.SwitchName
		$tb_ParentDriveFolder.Text = $MyMainConfig.ParentFolderPath
		$tb_ChildDiskPath.Text = $MyMainConfig.ChildRootFolder
		$tb_ISOFolderPath.Text = $MyMainConfig.ISOFolderPath
		
		if ($MyMainConfig.SSMSISO)
		{
			$cb_SSMSISO.Text = $MyMainConfig.SSMSISO
		}
		if ($MyMainConfig.DeveloperMachine)
		{
			$cb_DevMachine.Checked = $true
		}
		if ($MyMainConfig.ServerConfig)
		{
			$cb_DSCCentralHub.Checked = $true
			$tb_SQLServerFeatures.Text = $MySQLConfig.Features
		}
		
		if ($MyMainConfig.SQLServerISO)
		{
			
			$cb_SQLServerISO.Text = $MyMainConfig.SQLServerISO
		}
		
		if ($MyMainConfig.Windows2016ISO)
		{
			$cb_Windows2016ISO.Text = $MyMainConfig.Windows2016ISO
		}
		
		$ServersToLoop = @()
		$ServersToLoop = $MyMainConfig.ServerToQueue
		if (!$ServersToLoop)
		{
			$ServersToLoop = $MyMainConfig.DomainJoinServer.keys
		}
		$loopCnt = 0
		foreach ($server in $ServersToLoop)
		{
			
			if ($tb_ServerstoConfig.text)
			{
				$tb_ServerstoConfig.text = $tb_ServerstoConfig.text + ',' + $Server.Split("-", 2)[1]
			}
			else
			{ $tb_ServerstoConfig.text = $Server.Split("-", 2)[1] }
		}
		$cb_ConfigtoApply.SelectedItem = $MyMainConfig.ServerConfig
	}
}
$cb_SwitchType_SelectedIndexChanged = {
	if ($cb_SwitchType.SelectedItem -eq 'NAT')
	{
		$NatNet = $(get-netnat).InternalIPInterfaceAddressPrefix
		if ($NatNet)
		{
			$NatIp = "$($NatNet.Substring(0, $($NatNet.Length - 5))).1"
		}
		if ($NatIp)
		{
			$Natinterface = $(Get-NetIPAddress -IPAddress $NatIp).InterfaceAlias
		}
		
		$mySwitchName = $($Natinterface.Split("(")[1]).substring(0, $($($Natinterface.Split("(")[1]).length - 1))
		if ($NatNet -and $mySwitchName)
		{
			$tb_SwitchName.Text = $mySwitchName
			$newNetworkIPRange = "$($($mySwitchName.Substring(4, $($mySwitchName.Length - 6)))).x"
			$tb_NetworkIpRange.Text = $newNetworkIPRange
		}
		else
		{
			$tb_SwitchName.Enabled = $true
			$tb_NetworkIpRange.Enabled = $true
		}
		
	}
	if ($cb_SwitchType.SelectedItem -eq 'Private')
	{
		$tb_SwitchName.Enabled = $true
		$tb_NetworkIpRange.Enabled = $true
	}
}
