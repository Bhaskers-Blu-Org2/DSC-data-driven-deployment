			$PSCreatescript = $PSCreatescript + "New-DSCDataDrivenSQL -configuration `"$($InstallLocation)\LabConfig\$($tb_LabMachinePrefix.Text)\$($tb_LabMachinePrefix.Text)_Config.json`" -SQLConfiguration `"$InstallLocation\LabinaBox\LabConfig\$($tb_LabMachinePrefix.Text)\$($tb_LabMachinePrefix.Text)_SQLConfig.json`" -verbose `r"
			$PSCreatescript = $PSCreatescript + "Add-ServerConfigtoQueue -configuration `"$($InstallLocation)\LabConfig\$($tb_LabMachinePrefix.Text)\$($tb_LabMachinePrefix.Text)_Config.json`" -SQLConfiguration `"$InstallLocation\LabinaBox\LabConfig\$($tb_LabMachinePrefix.Text)\$($tb_LabMachinePrefix.Text)_SQLConfig.json`" -verbose"
			$ServersAddtoDomain = $ServersAddtoDomain + "$($tb_LabMachinePrefix.Text)-DSC"
		}
		
		if ($cb_AzureAutomation.Checked)
		{
			Add-Member -InputObject $sqlConfig -MemberType NoteProperty -Name "AzureCred" -Value "$($tb_LabMachinePrefix.Text)Administrator"
		}
		
		If (!(Test-Path -Path "$InstallLocation\LabConfig\$($tb_LabMachinePrefix.Text)"))
		{
			New-Item -ItemType Directory -Path "$InstallLocation\LabConfig\$($tb_LabMachinePrefix.Text)"
		}
		
		
		ConvertTo-Json -InputObject $sqlConfig | Out-File -FilePath "$($InstallLocation)\LabConfig\$($tb_LabMachinePrefix.Text)\$($tb_LabMachinePrefix.Text)_SQLConfig.json"
		
	}
	if ($cb_DevMachine.Checked -eq $true)
	{
		$ServersAddtoDomain = $ServersAddtoDomain + "$($tb_LabMachinePrefix.Text)-Dev"
	}
	Add-Member -InputObject $liabConfig -MemberType NoteProperty -Name 'DomainJoinServer' -Value $ServersAddtoDomain;
	$BailTest = !$formLabInABox.ValidateChildren()
	#$_.Cancel = !$formLabInABoxConfigurato.ValidateChildren()
	IF ($BailTest)
	{
		Show-Msbox -message "Validation Error Please fix." -icon "Critical"
	}
	else
	{
		New-Item -ItemType Directory -Force -Path "$InstallLocation\LabConfig\$($tb_LabMachinePrefix.Text)"
		ConvertTo-Json -InputObject $liabConfig | Out-File -FilePath "$InstallLocation\LabConfig\$($tb_LabMachinePrefix.Text)\$($tb_LabMachinePrefix.Text)_Config.json"
		$PSCreatescript | Out-File -FilePath "$InstallLocation\LabConfig\$($tb_LabMachinePrefix.Text)\$($tb_LabMachinePrefix.Text)_Create.ps1"
		$PSUpdatescript | Out-File -FilePath "$InstallLocation\LabConfig\$($tb_LabMachinePrefix.Text)\$($tb_LabMachinePrefix.Text)_Update.ps1"
		$PSStartScript | Out-File -FilePath "$InstallLocation\LabConfig\$($tb_LabMachinePrefix.Text)\$($tb_LabMachinePrefix.Text)_Start.ps1"
		$PSStopScript | Out-File -FilePath "$InstallLocation\LabConfig\$($tb_LabMachinePrefix.Text)\$($tb_LabMachinePrefix.Text)_Stop.ps1"
		$PSRemoveScript | Out-File -FilePath "$InstallLocation\LabConfig\$($tb_LabMachinePrefix.Text)\$($tb_LabMachinePrefix.Text)_Remove.ps1"
		$PSCheckPointScript | Out-File -FilePath "$InstallLocation\LabConfig\$($tb_LabMachinePrefix.Text)\$($tb_LabMachinePrefix.Text)_CheckPoint.ps1"
		$PSRemoveCheckPointScript | Out-File -FilePath "$InstallLocation\LabConfig\$($tb_LabMachinePrefix.Text)\$($tb_LabMachinePrefix.Text)_RemoveCheckPoint.ps1"
		
		if ($($openfiledialog.FileNames[0]) -eq "openfiledialog1") { $FinishText = "New Configuration Created" }
		else { $FinishText = "Configuration Updated" }
		$rc = Show-Msbox -message $FinishText
	}
	
	Invoke-Item "$InstallLocation\LabConfig\$($tb_LabMachinePrefix.Text)\"
	$formLabInABox.Close()
}
$button_Azure_Finish_Click = {
	$LabServers = $Serverdatagridview1.Rows | Select-Object -expand DataBoundItem | Select-Object Server
	$GridviewTable = $Serverdatagridview1.Rows | Select-Object -expand DataBoundItem | Select-Object Server,Azureoffer, AzureSku, VMUsername, VMPassword
	$domainJoinServer = [hashtable]@{ }
	foreach ($Server in $GridviewTable.server)
	{
		$domainJoinServer.Add($Server, @{ })
		$MyServerData = $Serverdatagridview1.rows | Select-Object -expand DataBoundItem | Where-Object { $_.Server -EQ $Server }
		$domainJoinServer.$($Server).Add("AzurePublisher", $MyServerData.AzurePublisher)
		$domainJoinServer.$($Server).Add("AzureOffer", $MyServerData.AzureOffer)
		$domainJoinServer.$($Server).Add("AzureSku", $MyServerData.AzureSku)
		$domainJoinServer.$($Server).Add("OS", $MyServerData.OS)
		$domainJoinServer.$($Server).Add("VMUsername", $MyServerData.VMUsername)
		$domainJoinServer.$($Server).Add("VMPassword", $MyServerData.VMPassword)
	}
	
	
	$AzureConfig = [PSCustomObject]@{
		'LabType'		  = 'AzureLab';
		'LabPrefix'	    = $tb_LabMachinePrefix.Text;
		'CertSubject'	  = $cb_CertificateList.SelectedItem;
		'ApplicationID'    = $tb_ApplicationID.Text;
		'TenantID'		 = $tb_TenantId.Text;
		'Size'			 = $tb_AzureVMSize.Text;
		'SubnetAddress'    = $tb_SubnetAddress.Text;
		'VNetAddress'	  = $tb_VnetAddress.Text;
		'Location'		 = $tb_AzureLocation.Text;
		'DomainJoinServer' = $domainJoinServer;
	}
	$PSMainscript = "Import-module -name `"$InstallLocation\modules\LabinaBox.psm1`"`r"
	$PSCreatescript = $PSMainscript + "New-AzureLab -configuration `"$InstallLocation\LabConfig\$($tb_LabMachinePrefix.Text)\$($tb_LabMachinePrefix.Text)_Config.json`" `r"
	$PSRemoveScript = $PSMainscript + "Remove-AzureLab -configuration `"$InstallLocation\LabConfig\$($tb_LabMachinePrefix.Text)\$($tb_LabMachinePrefix.Text)_Config.json`" `r"
	
	If (!(Test-Path -Path "$InstallLocation\LabConfig\$($tb_LabMachinePrefix.Text)"))
	{
		Set-Location c:\
		New-Item -ItemType Directory -Path "$InstallLocation\LabConfig\$($tb_LabMachinePrefix.Text)"
	}
	ConvertTo-Json -InputObject $AzureConfig | Out-File -FilePath "$InstallLocation\LabConfig\$($tb_LabMachinePrefix.Text)\$($tb_LabMachinePrefix.Text)_Config.json"
	$PSCreatescript | Out-File -FilePath "$InstallLocation\LabConfig\$($tb_LabMachinePrefix.Text)\$($tb_LabMachinePrefix.Text)_Create.ps1"
	$PSRemoveScript | Out-File -FilePath "$InstallLocation\LabConfig\$($tb_LabMachinePrefix.Text)\$($tb_LabMachinePrefix.Text)_Remove.ps1"
	
	Invoke-Item "$InstallLocation\LabConfig\$($tb_LabMachinePrefix.Text)\"
	$formLabInABox.Close()
	
}
